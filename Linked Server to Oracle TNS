USE [master]
GO

/****** Object:  LinkedServer [PMWDT]    Script Date: 10/17/2025 12:52:37 PM ******/
EXEC master.dbo.sp_addlinkedserver @server = N'PMWDT', @srvproduct=N'Oracle', @provider=N'OraOLEDB.Oracle', @datasrc=N'(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=hostname.org)(PORT=7693))(CONNECT_DATA=(SERVICE_NAME=pmwdt)))'
 /* For security reasons the linked server remote logins password is changed with ######## */
EXEC master.dbo.sp_addlinkedsrvlogin @rmtsrvname=N'PMWDT',@useself=N'False',@locallogin=NULL,@rmtuser=N'link',@rmtpassword='########'
GO

EXEC master.dbo.sp_serveroption @server=N'PMWDT', @optname=N'collation compatible', @optvalue=N'false'
GO

