-- efetuar Backup dos databases
EXECUTE dbo.DatabaseBackup
@Databases = 'ALL_DATABASES',
@Directory = 'H:\',
@BackupType = 'FULL',
@Verify = 'Y',
@Compress = 'Y',
@DatabasesInParallel = 'Y',
@NumberOfFiles = 6,
@CopyOnly = 'Y' 
